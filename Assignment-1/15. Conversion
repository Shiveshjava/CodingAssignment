mport java.util.Scanner;

public class day3farenheittocelcius {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int min =sc.nextInt();
        int max =sc.nextInt();
        int step = sc.nextInt();
       ;
        while(min<=max){
            int c = (5*(min-32))/9;
            System.out.println(min + "-"+ c);
            min=min+20;
        }
    }
}

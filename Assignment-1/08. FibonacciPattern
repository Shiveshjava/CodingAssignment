import java.util.Scanner;
public class day14Fibonaccipattern {
public static void main(String[] args) {
    

    // TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i=1;
        int nst =1;
        int a=0;
        int b=1;
        int c;
        while (i<=n) {
            int cst=0;
            while (cst<nst) {
                c=a+b;
                System.out.print(a+" ");
                a=b;
                b=c;
                cst++;
                
            }
System.out.println();
i++;
nst++;
            
        }
		
	}
}

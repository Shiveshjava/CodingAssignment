import java.util.Scanner;
public class day1pattern2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n=sc.nextInt();
int i=1;
int nst =1;
int count =1;
while(i<=n){
int cst=0;
while(cst<nst){
    System.out.print(count + "  " );
    cst++;
    count++;
}
System.out.println();

nst++;
i++;
;}
    }
}

package practice;
class practiceJava {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	int arr[]= {1,2,3,4,5};
	int target =3;
	int start =0;
	int end =arr.length-1;
	while(start<=end) {
		int mid=(start+end)/2;
		if(arr[mid]==target) {
			
			System.out.println("found at "+ mid+" ");
			break;
		}
			
		
			else if(arr[mid]<target) {
			start =mid+1;
		}
			else  {
			end=mid-1;
			}
	}
	
	
		
	
	}
	}



OUTPUT
found at 2 


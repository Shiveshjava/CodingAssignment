package practice;
import java.util.Stack;
import java .util.ArrayList;
import java.util.Collections;
class practiceJava {
	public static void main(String[] args) {
int arr[]= {9,8,7,6,5,4,3,2,1};
int start =0;
int end =arr.length;
int target =8;

while(start<end) {
	int mid=(start+end)/2;
	if(arr[mid]==target) {
		System.out.println("found at:"+ mid);
		return;
	}
	else if(arr[mid]>target) {
		start=mid+1;
		
	}
	else
		end =mid-1;
		
	
}
	
	
		
	}

}
OUTPUT:
found at:1

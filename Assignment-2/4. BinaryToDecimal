import java.util.Scanner;
public class day15Chewbecca {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int n =sc.nextInt();
      int ans=0;
      int index =0;
    
      while (n>0) {
        int temp = n%10;
        ans =ans+(int)Math.pow(2, index) * temp;
        index= index+1;
        
        
        n =n/10;

      }
      System.out.println(ans);

}}

import java.util.Scanner;
public class day15Chewbecca {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int N1 =sc.nextInt();
      int N2 =sc.nextInt();
      int n=1;
      int n1=1;



      while(n>0){
int S = 3*n +2;

if(S%N2!=0){
    System.out.println(S);
n1++;
}
if(n1>N1){
    break;
}
        n++;
      }
    }

}

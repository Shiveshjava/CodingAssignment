import java.util.Scanner;
public class day15Chewbecca {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);

int index=1;
int osum=0;
int esum=0;

int n =sc.nextInt();
while(n>0){
    int digit = n%10;
if(index%2==0){
    esum =esum+digit;
    index++;
    n =n/10;

}
else{
    osum = osum+digit;
    index++;
    n=n/10;
}
}
System.out.println(osum);
System.out.println(esum);


    }
}
